         ['2015',  14852,      38464,    43960,          977],
         ['2016',  13488,      34676,    39851,          1140],
      ]);

    var options2 = {
      title : 'terrorism info by year',
      vAxis: {title: 'number'},
      hAxis: {title: 'Year'},
      seriesType: 'bars',
      series: {5: {type: 'line'}}
    };

    var chart2 = new google.visualization.ComboChart(document.getElementById('chart_div2'));
    chart2.draw(data2, options2);
  }
    </script>
